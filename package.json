  "scripts": {
    "jsonlint": "npm run --silent jsonlint:files | xargs -n1 jsonlint -q -c && echo \"jsonlint: no lint errors\"",
    "jsonlint:files": "find . -name \"*.json\" | grep -v -f .eslintignore",
    "lint": "eslint . --cache && echo \"eslint: no lint errors\"",
    "lint:fix": "eslint . --cache --fix && echo \"eslint: no lint errors\"",
    "prettier": "npm run --silent prettier:files | xargs prettier --write",
    "prettier:files": "find . -name \"*.js\" -o -name \"*.d.ts\" -o -name \"*.tsx\" | grep -v -f .eslintignore",
    "prettier:check": "npm run --silent prettier:files | xargs prettier --list-different",
    "size": "size-limit",
    "size:why": "size-limit --why build/index.js",
    "size:overhead:why": "size-limit --why ./test/size/overhead.js",
    "test": "node scripts/test-unit.js --env=jsdom --showConfig --debug",
    "prebuild": "rimraf build",
    "build:es2015": "cross-env NODE_ENV=production babel --config-file ./babel.config.js ./src --out-dir ./build --ignore *.test.js",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel --config-file ./babel.config.js ./src/index.js --out-file ./build/index.es.js",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config.js ./src --out-dir ./build/es --ignore *.test.js",
    "build:umd": "cross-env BABEL_ENV=production-umd rollup -c scripts/rollup.config.js",
    "build:copy-files": "babel-node --config-file ./babel.config.js ./scripts/copy-files.js",
    "build": "npm run build:es2015 && npm run build:es2015modules && npm run build:es && npm run build:copy-files",
    "release": "npm run build",
    "storybook": "start-storybook -p 8999 -c config/.storybook",
    "build-storybook": "NODE_ENV=development build-storybook -c config/.storybook -o docs",
    "storybook:static": "npx http-server docs",
    "docker:build": "docker build -t storybook:$npm_package_version .",
    "docker:interactive": "docker run -it -p 8080:8080 --rm storybook:$npm_package_version bash",
    "watch": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config.js ./src --out-dir ./build/es --ignore *.test.js --watch",
    "deploy": "node scripts/cdnify.js -b ux.amsarmada.com -r us-west-2",
    "prepare": "npm run build"
  },
