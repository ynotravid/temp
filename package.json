  "nyc": {
    "include": [
      "packages/ams-ui/src/**/*.js"
    ],
    "exclude": [
      "**/*.test.js"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>config/jest/init.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "size-limit": [
    {
      "path": "./build/index.js",
      "limit": "9 KB"
    }
  ],
  "browser": {
    "Notifier": "./build/ducks/Notifier/Notifier.js",
    "NotifierReduxProxy": "./build/ducks/Notifier/NotifierReduxProxy.js",
    "notifierActions": "./build/ducks/Notifier/actions.js",
    "notifierReducer": "./build/ducks/Notifier/index.js"
  },
  "main": "./build/index.js",
  "module": "./build/es/index.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "private"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "files": [
    "./build",
    "./src"
  ],
  "resolutions": {
    "**/event-stream": "3.3.4"
  }
